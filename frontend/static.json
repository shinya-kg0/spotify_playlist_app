{
    "root": "dist/",
    "clean_urls": false,
    "routes": {
        "/**": "index.html"
    },
    "headers": {
        "/static/**": {
        "Cache-Control": "public, max-age=31536000, immutable"
        },
        "/assets/**": {
        "Cache-Control": "public, max-age=31536000, immutable"
        }
    },
    "https_only": true,
    "error_page": "index.html"
}